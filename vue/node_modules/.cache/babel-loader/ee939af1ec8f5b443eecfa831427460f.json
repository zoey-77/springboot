     <el-form-item prop=\"description\" label=\"申请说明\">\r\n          <el-input type=\"textarea\" v-model=\"form.description\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"join\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"BlogDetail\",\r\n  data() {\r\n    return {\r\n      id: this.$route.query.id,\r\n      activity: {},\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      dialogFormVisible: false,\r\n      form: {}\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    handleJoin() {\r\n      this.dialogFormVisible = true\r\n    },\r\n    join() {\r\n      let data = {\r\n        name: this.activity.name,\r\n        description: this.form.description,\r\n        user: this.user.username,\r\n        activityId: this.activity.id,\r\n        state: '待审核',\r\n      }\r\n      this.request.post(\"/activityApply\", data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"报名成功，请等待审核\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n        this.form = {}\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/activity/\" + this.id).then(res => {\r\n        this.activity = res.data\r\n      })\r\n    },\r\n    sign(){\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.item1 {\r\n  color: #666;\r\n}\r\n</style>"],"sourceRoot":"src/views/front"}]}