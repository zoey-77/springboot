,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAPA;AAQA,KAxEA;AAyEA,IAAA,qBAzEA,iCAyEA,GAzEA,EAyEA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KA5EA;AA6EA,IAAA,QA7EA,sBA6EA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,0BAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KA3FA;AA4FA,IAAA,KA5FA,mBA4FA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KA/FA;AAgGA,IAAA,gBAhGA,4BAgGA,QAhGA,EAgGA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KApGA;AAqGA,IAAA,mBArGA,+BAqGA,OArGA,EAqGA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KAzGA;AA0GA,IAAA,uBA1GA,mCA0GA,GA1GA,EA0GA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KA5GA;AA6GA,IAAA,sBA7GA,kCA6GA,GA7GA,EA6GA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KA/GA;AAgHA,IAAA,QAhHA,oBAgHA,GAhHA,EAgHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAlHA;AAmHA,IAAA,GAnHA,iBAmHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,4CAAA;AACA,KArHA;AAsHA,IAAA,wBAtHA,sCAsHA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AAzHA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-card>\r\n      <el-table :data=\"tableData\">\r\n        <el-table-column prop=\"name\" label=\"姓名\"></el-table-column>\r\n        <el-table-column prop=\"description\" label=\"申请说明\"></el-table-column>\r\n        <el-table-column prop=\"department.name\" label=\"社团名称\"></el-table-column>\r\n        <el-table-column prop=\"activity.name\" label=\"活动名称\"></el-table-column>\r\n        <el-table-column prop=\"reviewer\" label=\"审核人姓名\"></el-table-column>\r\n        <el-table-column prop=\"state\" label=\"审核状态\"></el-table-column>\r\n        <el-table-column prop=\"reason\" label=\"审核说明\"></el-table-column>\r\n\r\n        <el-table-column label=\"操作\"  width=\"180\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" @click=\"cancel(scope.row)\" v-if=\"user.role == 'ROLE_USER' && scope.row.state == '待审核'\">取消</el-button>\r\n            <el-popconfirm\r\n                class=\"ml-5\"\r\n                confirm-button-text='确定'\r\n                cancel-button-text='我再想想'\r\n                icon=\"el-icon-info\"\r\n                icon-color=\"red\"\r\n                title=\"您确定删除吗？\"\r\n                @confirm=\"del(scope.row.id)\"\r\n            >\r\n              <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div style=\"padding: 10px 0\">\r\n        <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[2, 5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-card>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"140px\" size=\"small\" style=\"width: 95%;\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n        <el-form-item prop=\"reviewer\" label=\"审核人姓名\">\r\n          <el-input v-model=\"form.reviewer\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"state\" label=\"审核状态\">\r\n          <template>\r\n            <el-radio v-model=\"form.state\" label=\"审核通过\">审核通过</el-radio>\r\n            <el-radio v-model=\"form.state\" label=\"审核不通过\">审核不通过</el-radio>\r\n          </template>\r\n        </el-form-item>\r\n        <el-form-item prop=\"reason\" label=\"审核说明\">\r\n          <el-input type=\"textarea\" v-model=\"form.reason\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"activityApply\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入名称', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    cancel(o){\r\n      o.state = '已取消'\r\n      this.request.post(\"/activityApply\", o).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"取消成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/activityApply/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n        this.$refs['ruleForm'].validate((valid) => {\r\n          if (valid) {\r\n            this.request.post(\"/activityApply\", this.form).then(res => {\r\n              if (res.code === '200') {\r\n                this.$message.success(\"保存成功\")\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n              } else {\r\n                this.$message.error(res.msg)\r\n              }\r\n            })\r\n          }\r\n        })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/activityApply/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/activityApply/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(\"http://localhost:9090/activityApply/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}